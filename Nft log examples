Sub ChangeCheckboxToEmpty()
    Dim cc As ContentControl
    
    ' Loop through all content controls in the document
    For Each cc In ActiveDocument.ContentControls
        ' Check if the content control is a checkbox
        If cc.Type = wdContentControlCheckBox Then
            ' If the checkbox is checked or unchecked, set it to unchecked (empty)
            cc.Checked = False
        End If
    Next cc
End Sub
